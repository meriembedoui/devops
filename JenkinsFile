pipeline {
    agent any

    tools {
        maven 'maven3'
        jdk 'java21'
    }
    stages {
        stage('Clone repository') {
            steps {
                git branch: 'master',
                    url: 'https://github.com/meriembedoui/devops.git'
            }
        }
  stage('Build') {
            steps {
                sh 'mvn -B clean package'
            }
        }
        
   }   
}
